{
  "name": "react-movie-app",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test:unit": "react-app-rewired test",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eject": "react-scripts eject",
    "ec:write": "editorconfig-checker",
    "lint:write": "eslint src/. --fix",
    "prettier:write": "prettier --write \"src/**/*.+(jsx|js|json|md)\"",
    "analyze": "react-app-rewired build --analyze",
    "deploy": "now"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flickity-component": "^3.2.0",
    "react-lazy-load-image-component": "^1.4.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.1.0",
    "babel-eslint": "^10.0.3",
    "customize-cra": "^0.9.1",
    "editorconfig-checker": "^3.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.5",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.3.0",
    "sass-resources-loader": "^2.0.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo-loader": "^2.2.1",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
